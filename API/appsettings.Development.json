{
  "AllowedHosts": "*",

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {},
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp}{NewLine}{Properties}{NewLine}{Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "SinkRabbitMQ"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadName" ]
  },
  "Middleware": {
    "SinkRabbitMQ": {
      "Hostnames": "",
      "Username": "",
      "Password": "",
      "Exchange": "",
      "ExchangeType": "fanout",
      "Durable": true,
      "RouteKey": "",
      "ExchangeLogger": "",
      "RouteKeyLogger": "",
      "QueueLogger": "LogApps-exchange",
      "AutoDelete": false,
      "Port": 0
    },
    "RabbitMQ": {
      "Host": "<host rabbitMQ>",
      "Username": "<user rabbitMQ>",
      "Password": "<password rabbitMQ>"
    }
  }
}
